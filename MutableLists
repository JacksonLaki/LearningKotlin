val fruits:MutableList<String> = mutableListOf("Banana", "Mango", "Apple")

fun main() {
    
    
   // println("Size of fruits is ${fruits.size}")
    
    //printing all list of fruits using foreach Loop.package
    
    fruits.forEach(){
        
        fruit->println(fruit)
    }
    
    fruits.add("orange")
    
    println("Size of fruits is ${fruits.size}")
    
      fruits.forEach(){
        
        fruit->println(fruit)
    }
  
}
