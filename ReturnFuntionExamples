RETURN TYPE ONE

fun main() {
  val total = getAddition(1,1)
  println("$total")
}

fun getAddition(num1: Int,num2:Int):Int {
    return num1 + num2
    
}

RETURN TYPE TWO

fun main() {
  println("${getAddition(1,1)}")
}

fun getAddition(num1: Int,num2:Int):Int {
    return num1 + num2
    
}

RETURN TYPE THREE (STRINGS)

fun main() {
    greetingCard()
    giveThanks()
}


fun greetingCard(){
    
    print("Welcome!")
}

fun giveThanks(){
    
    print("Thank You!")
}
