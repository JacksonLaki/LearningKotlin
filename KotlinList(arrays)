val fruits:List<String> = listOf("Banana", "Mango", "Apple")

fun main() {
    
println("fruits.size)

//print with String.
println("Size of fruits is ${fruits.size}")

   //printing all list of fruits using foreach Loop.package
    
    fruits.forEach(){
        
        fruit->println(fruit)
    }
  
}
